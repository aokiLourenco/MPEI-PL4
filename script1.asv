clear all;
clc;

movies = readcell('movies.csv', 'Delimiter', ',');

genres = getGenres(movies);
fprintf(genres);

function genres = getGenres(movies)
    genres = {};
    k = 1;

    for i = 1:height(movies)
        for j = 2:7
            if ~anymissing(movies{i, j}) && ~strcmp(movies{i, j}, 'unkown')
                genres{k} = movies{i, j};
                k = k + 1;
            end
        end
    end

    end
